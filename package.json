{
  "name": "tiny-universal",
  "version": "0.0.17",
  "description": "A universal router for isomorphic React applications",
  "main": "dist/tiny-universal.js",
  "repository": "git@github.com:craigcollie/react-universal-router.git",
  "author": "Craig Collie <hello@craigcollie.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn watch",
    "watch": "node -r babel-register node_modules/.bin/webpack --config=./webpack.config --watch",
    "test": "yarn mocha && yarn eslint",
    "eslint": "eslint --quiet './src/**/*.js'",
    "mocha": "mocha --opts .mocharc 'src/**/*-spec.js'",
    "mochaSingle": "mocha --opts .mocharc src/**/$test*-spec.js -w",
    "bump": "npm version patch && npm publish",
    "bundle": "rm -rf dist && mkdir dist && yarn package && yarn bump",
    "package": "node -r babel-register node_modules/.bin/webpack -p --config=./webpack.config"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "9.12.0",
    "jsdom-global": "^2.1.1",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "sinon": "^2.1.0",
    "webpack": "^2.5.1",
    "webpack-node-externals": "^1.5.4"
  }
}
